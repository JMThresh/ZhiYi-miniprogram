.container {
  margin: 20rpx 30rpx 0;
  height: 100vh;
}
.container .search_container {
  background-color: #f8f8f8;
  border-radius: 30rpx;
}
.container .search_container l-input {
  flex: 1;
}
.container .search_container l-input l-button .l-class {
  border-radius: 20rpx;
}
.container .provinces_container {
  margin-top: 30rpx ;
}
.container .provinces_container .province .title {
  font-size: 45rpx;
}
.container .provinces_container .province .city_box {
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx ;
}
.container .provinces_container .province .city_box .city {
  width: 150rpx ;
  padding: 10rpx 3rpx;
  margin: 0 22rpx 22rpx 0;
  text-align: center;
  border-radius: 20rpx;
  background-color: #f4f4f4;
  font-weight: 500;
  font-size: 32rpx;
}
