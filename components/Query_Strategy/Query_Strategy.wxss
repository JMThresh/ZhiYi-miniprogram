.container {
  padding: 30rpx;
  background-color: #f9f9f9;
  height: 100vh;
}
.container .travel_strategy {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 30rpx  20rpx;
  height: 120rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.container .travel_strategy .txt {
  flex: 1;
  text-align: center;
  font-weight: bold;
  font-family: "STZhongsong";
  font-size: 35rpx;
}
.container .travel_strategy .from_to_container {
  flex: 2;
  display: flex;
  background-color: #f4f4f4;
  margin: 0  20rpx 0 10rpx;
  height: 70rpx;
  border-radius: 20rpx;
}
.container .travel_strategy .from_to_container .city {
  flex: 4;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  font-weight: bold;
}
.container .travel_strategy .from_to_container .change {
  flex: 1;
  font-size: 40rpx;
  color: #333;
  display: flex;
  justify-content: center;
  align-items: center;
}
.container .travel_strategy .from_to_container .fontGrey {
  color: #9b9999;
}
.container .travel_strategy .l-class {
  border-radius: 20rpx;
  font-size: 40rpx;
}
